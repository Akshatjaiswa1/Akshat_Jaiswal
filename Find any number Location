import phonenumbers
from phonenumbers import geocoder
phone_number1 = phonenumbers.parse("+918076XXXXXX")     #place of +91 write country code example +121 etc#
phone_number2 = phonenumbers.parse("+1218076XXXXXX")

("\nPhone Number Location\n")
print(geocoder.description_for_number(phone_number1,"en"));
print(geocoder.description_for_number(phone_number2,"en"));
